{
	"settings": {
		"ecmascript": 6,
		"jsx": true
	},{
		"globals": {
			"jQuery": false,
			"$": true,
			"require": true,
			"Backbone": true,
			"Fmk": true,
			"_": true,
			"Promise": true,
			"module": true
		},
		"env": {
			"browser": true,
			"node": true
		},
		"rules": {
			"valid-jsdoc": [2, {
				"requireParamDescription": true
			}],
			"quotes": [0]
		}
	}
}
